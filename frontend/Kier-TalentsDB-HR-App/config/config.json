{
  "$schema": "https://dev.office.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "talent-record-forms-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/talentRecordForms/TalentRecordFormsWebPart.js",
          "manifest": "./src/webparts/talentRecordForms/TalentRecordFormsWebPart.manifest.json"
        }
      ]
    },
    "talent-record-time-line-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/talentRecordTimeLine/TalentRecordTimeLineWebPart.js",
          "manifest": "./src/webparts/talentRecordTimeLine/TalentRecordTimeLineWebPart.manifest.json"
        }
      ]
    },
    "talent-portal-command-set-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/talentPortalCommandSet/TalentPortalCommandSetCommandSet.js",
          "manifest": "./src/extensions/talentPortalCommandSet/TalentPortalCommandSetCommandSet.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "TalentRecordFormsWebPartStrings": "lib/webparts/talentRecordForms/loc/{locale}.js",
    "TalentRecordTimeLineWebPartStrings": "lib/webparts/talentRecordTimeLine/loc/{locale}.js",
    "TalentPortalCommandSetCommandSetStrings": "lib/extensions/talentPortalCommandSet/loc/{locale}.js"
  }
}
